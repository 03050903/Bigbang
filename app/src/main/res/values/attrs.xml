<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BigBangLayout">
        <attr name="itemSpace" format="dimension"/>
        <attr name="lineSpace" format="dimension"/>
        <attr name="textSize" format="dimension"/>
        <attr name="textColor" format="reference"/>
        <attr name="textBackground" format="reference"/>
        <attr name="sectionTextBackground" format="reference"/>
    </declare-styleable>
    <attr name="fromDegrees" format="float|reference" />
    <attr name="toDegrees" format="float|reference" />
    <attr name="childSize" format="dimension|reference" />

    <declare-styleable name="ArcLayout">
        <attr name="fromDegrees"/>
        <attr name="toDegrees"/>
        <attr name="childSize"/>
    </declare-styleable>
</resources>